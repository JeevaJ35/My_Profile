# Allowed CORS origins (comma-separated). Include your Vercel/Netlify URL.
CORS_ORIGIN=http://localhost:5173

# SMTP settings (use your provider or Gmail with App Password)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# From and To (where the contact messages go)
FROM_EMAIL=your_email@gmail.com
TO_EMAIL=your_email@gmail.com

# Server
PORT=8080
